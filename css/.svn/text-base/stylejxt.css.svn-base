body{
	background: #f1f1f1;
	font-family: "微软雅黑";
}
img{
	border: none 0;
}
ul{
	margin: 0;
	padding: 0;
}
li{
	list-style: none;
}
p{
	margin: 0;
}
/**
 * 圆角头像
 */
.round-head{
	border-radius: 8px;
}

/*圆形头像*/
.circular{
	width:100px;
	height:100px;
	border-radius:50%;
	}
/*登录*/
.login{
	padding-top: 10px;
}
.login input{
	width: 100%;
	border-radius: 0;
	border-left: none 0;
	border-right: none 0;
	height: 46px;
	font-family: "微软雅黑";
	font-size: 16px;
}
.login .login-none{
	border-bottom: none 0;
}
#login-but{
	width: 100%;
	background: #ac1a1a;
	color: #fff;
	font-size: 18px;
	border-radius: 10px;
	border: none 0;
	height: 46px;
	margin-top: 50px;
}
/*登录*/

/*首页*/
.mui-bar-nav img{
	width: 24px;
	height: 24px;
	border-radius: 12px;
	margin-top: 10px;
	margin-right: 40px;
	cursor: pointer;
}
.mui-bar-nav .per_center{
	display: inline-block;
	padding-right: 40px;
	cursor: pointer;
}
.news{
	overflow: hidden;
	background: #d7d5d5;
	padding: 0px 10px;
}
.news img{
	width: 18px;
	float: left;
	margin-top: 10px;
}
.news p{
	color: #222;
	font-size: 16px;
	margin: 0;
	line-height: 18px;
	cursor: pointer;
}
.mui-slider img{
	width: 100%;
	height: auto;
}
.index-nav{
	width: 100%;
	background: #ac1a1a;
	overflow: hidden;
	padding: 10px 10px;
}
.index-nav ul{
	overflow: hidden;
}
.index-nav li{
	width: 30.1111%;
	float: left;
	text-align: center;
	background-color: rgba(215,213,213,.3);
	margin: 1.6%;
	border-radius: 10px;
	padding: 3% 0;
}
.index-nav li span{
	margin: 3% 0;
}
.index-nav li p{
	color: #f5f5f5;
}
/*首页*/

/*详情页*/
#jiaoliu img{
	width: 46px;
	float: left;
	margin-right: 10px;
}
#jiaoliu .pinglun{
	width: 18px;
	position: absolute;
	right: 20px;
	top: 40px;
	margin-right: 0;
}
#jiaoliu ul{
	overflow: hidden;
}
#jiaoliu li{
	font-size: 16px;
	color: #333;
	overflow: hidden;
}
#jiaoliu li label{
	padding: 10px 0;
	display: block;
	border-bottom: 1px solid #eee;
}
#jiaoliu li img{
	width: 30px;
	margin: 15px 0px 0 0;
}
#jiaoliu li p{
	padding: 10px 0;
	border-bottom: 1px solid #eee;
	margin-left: 40px;
	font-size: 16px;
	color: #333;
}
#jiaoliu li span{
	display: block;
	font-size: 14px;
	color: #999;
}
